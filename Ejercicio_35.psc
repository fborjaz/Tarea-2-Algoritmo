Algoritmo sin_titulo
	Definir num1, num2 Como Entero;
	Escribir 'Ingresa el número 1';
	leer num1;
	Escribir 'Ingresa el número 2';
	leer num2;
	si num1 mod num2 == 0 Entonces
		
		Escribir 'El número ',num1,' es divisible entre ',num2;
		
	SiNo
		
		Escribir 'El número ',num1,' no es divisible entre ',num2;
		
	FinSi
	
FinAlgoritmo
