Algoritmo Calcular_X
	Definir X Como Real;
	Definir N, Poten,Cont Como Entero;
	
    Poten = 0;
	
	Escribir 'Introducir un numero: ';
	Leer X;
	
	Escribir 'Introducir el exponente: ';
	Leer N;
	
	Cont = X ^N;
	Poten = Cont;
	
	Escribir 'El ',X,' con exponete ',N,' es: ',Poten;
FinAlgoritmo
