Algoritmo Cuadrado_men0
	Definir Cua,Num Como Entero;
	
	Num = 1;
	
	Mientras Num > 0 Hacer
		Escribir 'Ingrese un numero numero';
		Leer Num;
		
		Cua = Num * Num;
		
		si Num > 0 Entonces
			
			Escribir 'El cuadrado de ',Num,' es: ',Cua;
			
		FinSi
		
	FinMientras
	
FinAlgoritmo
